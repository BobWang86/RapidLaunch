<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - RapidLaunch</title>
    <!-- Favicons -->
    <link href="~/favicon.ico" rel="icon" />
    <!-- Bootstrap core CSS -->
    <link href="~/lib/bootstrap_copy/css/bootstrap.min.css" rel="stylesheet">
    <!--External CSS-->
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/css/bootstrap-select.min.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/style-responsive.css" rel="stylesheet">

    <!--common script for all pages-->
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery_copy/jquery.min.js"></script>
    <script src="~/lib/bootstrap_copy/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap-select.min.js"></script>
    <script src="~/lib/jquery.dcjqaccordion.2.7.js" class="include"></script>
    <script src="~/lib/jquery.scrollTo.min.js"></script>
    <script src="~/lib/jquery.nicescroll.js"></script>
    <script src="~/lib/common-scripts.js"></script>
</head>
<body>
    <section id="container">
        <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        @*<partial name="_CookieConsentPartial" />*@

        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars"></div>
            </div>
            <!--logo start-->
            <a asp-area="" asp-controller="Home" asp-action="Index" class="logo"><b>Rapid<span>Launch</span></b></a>
            <!--logo end-->
            <div class="top-menu">
                <partial name="_LoginPartial">
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <br />
        <br />
        <br />
        <section class="wrapper">
            @RenderBody()
        </section>
        <!--main content end-->
    </section>

    @RenderSection("Scripts", required: false)
</body>
</html>